{
	"name": "@nihility-io/cookies",
	"version": "2.0.0-alpha.8",
	"exports": "./mod.ts",
	"lock": true,
	"tasks": {},
	"imports": {
		"js-cookie": "npm:js-cookie@^3.0.5",
		"preact": "npm:preact@^10.26.5",
		"preact/": "npm:/preact@^10.26.5/"
	},
	"lint": {
		"rules": {
			"tags": ["recommended", "jsr"],
			"include": [
				"jsx-button-has-type",
				"camelcase",
				"default-param-last",
				"eqeqeq",
				"explicit-function-return-type",
				"explicit-module-boundary-types",
				"no-const-assign",
				"no-eval",
				"no-external-import",
				"no-inferrable-types",
				"no-self-compare",
				"no-sparse-arrays",
				"no-sync-fn-in-async-fn",
				"no-throw-literal",
				"no-undef",
				"no-useless-rename",
				"single-var-declarator"
			],
			"exclude": [
				"verbatim-module-syntax"
			]
		}
	},
	"fmt": {
		"useTabs": true,
		"lineWidth": 120,
		"indentWidth": 4,
		"semiColons": false,
		"singleQuote": false,
		"proseWrap": "preserve"
	},
	"compilerOptions": {
		"lib": [
			"deno.window"
		]
	}
}
